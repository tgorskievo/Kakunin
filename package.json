    "moment": "^2.18.1",
    "node-env-file": "^0.1.8",
    "node-fetch": "^2.1.2",
    "node-xlsx": "^0.12.0",
    "path": "^0.12.7",
    "protractor": "^5.3.2",
    "protractor-cucumber-framework": "^6.1.1",
    "protractor-multiple-cucumber-html-reporter-plugin": "^1.2.3",
